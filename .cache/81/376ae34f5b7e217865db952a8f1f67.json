source":"style/index.css","original":{"line":268,"column":0},"generated":{"line":268,"column":0}},{"source":"style/index.css","original":{"line":269,"column":0},"generated":{"line":269,"column":0}},{"source":"style/index.css","original":{"line":270,"column":0},"generated":{"line":270,"column":0}},{"source":"style/index.css","original":{"line":271,"column":0},"generated":{"line":271,"column":0}},{"source":"style/index.css","original":{"line":272,"column":0},"generated":{"line":272,"column":0}},{"source":"style/index.css","original":{"line":273,"column":0},"generated":{"line":273,"column":0}}],"sources":{"style/index.css":"* {\n    font-size: 62.5%;\n    max-width: 100vw;\n    max-height: auto;\n    box-sizing: border-box;\n}\n\nbody {\n    width: 100%;\n    height: auto;\n    margin: 0 auto;\n    font-family: 'Ubuntu', sans-serif;\n    color: #FAF9F6;\n    background-color: #5E5768;\n}\n\n/* top page elements */\nheader {\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n    flex-flow: row wrap;\n    background-color: #8FA6CB;\n}\n\nheader h1 {\n    color: #EE964B;\n    font-size: 5rem;\n}\n\nh1,\nh2 {\n    font-family: 'Lobster', cursive;\n    text-shadow: .2rem .2rem #50514F;\n    font-size: 4rem;\n}\n\nh3 {\n    color: #EE964B;\n    font-family: 'Lobster', cursive;\n    font-size: 3rem;\n}\n\np {\n    font-size: 2rem;\n    font-family: 'Ubuntu', sans-serif;\n}\n\nnav a {\n    font-family: 'Lobster', cursive;\n    text-decoration: none;\n    color: #EE964B;\n    text-shadow: .2rem .2rem #50514F;\n    font-size: 2.5rem;\n    padding: 1rem;\n    /* background-color: white; */\n    /* border: solid 2px gray; */\n    /* border-radius: 10px; */\n}\n\nnav a:hover {\n    /* background-color: #a7c7e7; */\n    color: #D4DF9E;\n    transition: .3s;\n}\n\n/* this is where the slideshow gallery goes */\n\n/* slideshow container */\n.slideshow-container {\n    height: auto;\n    max-width: 100rem;\n    position: relative;\n    padding-top: 3rem;\n    margin: auto;\n}\n\n/* hides the images by default */\n.mySlides {\n    display: flex;\n    justify-content: center;\n    text-align: center;\n    align-items: center;\n    height: fit-content;\n    width: fit-content;\n    display: none;\n}\n\n.mySlides img{\n    border-radius: 1rem;\n}\n\n/* next and prev buttons */\n.next,\n.prev {\n    cursor: pointer;\n    position: absolute;\n    top: 50%;\n    width: auto;\n    margin-top: -2.2rem;\n    padding: 1.6rem;\n    color: white;\n    font-weight: bold;\n    font-size: 1.8rem;\n    transition: 0.6s ease;\n    border-radius: 0rem .3rem .3rem 0rem;\n    user-select: none;\n}\n\n/* position the \"next\" button to the right */\n.next {\n    right: 0;\n    border-radius: .3rem 0rem 0rem .3rem;\n}\n\n/* On hover, add a black background color with a little bit see-through */\n.next:hover,\n.prev:hover {\n    background-color: rgba(0, 0, 0, 0.8);\n}\n\n/* caption text */\n.text {\n    color: #f2f2f2;\n    text-shadow: .2rem .2rem #50514f;\n    font-size: 1.5rem;\n    padding: .8rem 1.2rem;\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    font-size: 2rem;\n    width: fit-content;\n    background-color: #50514f;\n    border-radius: 1rem;\n}\n\n/* number text (1/3 etc) */\n.numbertext {\n    color: #f2f2f2;\n    font-size: 1.2rem;\n    padding: .8rem 1.2rem;\n    position: absolute;\n    top: 3rem;\n}\n\n/* the dots/bullets/indicators */\n.dot {\n    cursor: pointer;\n    height: 1.5rem;\n    margin: 0 .2rem;\n    background-color: #bbb;\n    border-radius: 50%;\n    display: inline-block;\n    transition: background-color 0.6s ease;\n}\n\n.active,\n.dot:hover {\n    background-color: #717171;\n}\n\n/* fading animation */\n.fade {\n    -webkit-animation-name: fade;\n    -webkit-animation-duration: 1.5s;\n    animation-name: fade;\n    animation-duration: 1.5s;\n}\n\n@-webkit-keyframes fade {\n    from {\n        opacity: .4\n    }\n\n    to {\n        opacity: 1\n    }\n}\n\n@keyframes fade {\n    from {\n        opacity: .4\n    }\n\n    to {\n        opacity: 1\n    }\n}\n\n/* footer section elements */\nfooter {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 100%;\n    height: 5rem;\n    background-color: #8FA6CB;\n    margin: 0 auto;\n}\n\nfooter a:hover {\n    color: #D4DF9E;\n    transition: .3s;\n}\n\n.closing {\n    color: #EE964B;\n    font-family: 'Lobster', cursive;\n    font-size: 2.5rem;\n    text-decoration: none;\n    text-shadow: .1rem .1rem black;\n    ;\n}\n\n@media(max-width: 800px) {\n    header h1 {\n        max-width: 100vw;\n        height: auto;\n        margin: 0;\n        padding-bottom: 2rem;\n    }\n\n    body {\n        margin: 0 auto;\n    }\n\n    header {\n        display: block;\n        text-align: center;\n        margin: 0 auto;\n        padding: 2rem 0;\n    }\n\n    nav {\n        display: flex;\n        flex-flow: row wrap;\n        justify-content: center;\n        text-align: center;\n        align-items: center;\n    }\n\n    nav a {\n        width: 15rem;\n        margin: .5rem auto;\n        padding: 2rem;\n    }\n}\n\n@media(max-width: 500px) {\n\n    header {\n        height: 15rem;\n        align-items: center;\n        justify-content: center;\n    }\n\n    nav a {\n        padding: 1.5rem;\n        margin: 0 auto;\n        max-width: fit-content;\n    }\n\n    nav {\n        display: flex;\n        flex-wrap: wrap;\n        flex-direction: row;\n        max-width: 100vw;\n        height: 0;\n        margin: 0 auto;\n        padding: 1rem;\n    }\n}"},"lineCount":273}},"error":null,"hash":"a859af9da044490fcf4b755cbdc671d6","cacheData":{"env":{}}}